<% contentFor('body') %>
    <p>建立Project</p>
    <form action="/admin/project/create" method="post">
        <div>
            <label for="title">專案名稱</label>
            <input id="title" type="text" placeholder="請輸入專案名稱" name="title">
        </div>
        <div>
            <label for="demoUrl">展示網址</label>
            <input id="demoUrl" type="text" placeholder="請輸入專案Demo網址" name ="demoUrl">
        </div>
        <div>
            <label for="repoUrl">GitHub網址</label>
            <input id="repoUrl" type="text" placeholder="請輸入專案gitHub網址" name="repoUrl">
        </div>
        <textarea placeholder="請輸入專案特色描述" cols="30" rows="10" name="featureDescription"></textarea>
        <textarea placeholder="請輸入專案技術描述" cols="30" rows="10" name="technologyDescription"></textarea>
        <textarea placeholder="請輸入專案未來規劃" cols="30" rows="10" name="futureDescription"></textarea>
        <button type="submit">Create</button>
    </form>